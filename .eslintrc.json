{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            "never",
            {
                "js": "always"
            }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-restricted-syntax": [
            "error",
            "FunctionExpression",
            "WithStatement",
            "BinaryExpression[operator='in']"
        ],
        "no-restricted-globals": [
            "error",
            "event",
            "fdescribe"
        ],
        "import/prefer-default-export": false
    }
}
